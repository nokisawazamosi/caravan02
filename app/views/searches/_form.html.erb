<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :word %>
    <%= f.select :model, options_for_select({"User" => "user", "Blog" => "blog"}) %>
    <%= f.select :method, options_for_select({"完全一致" => "perfect", "前方一致" => "foward", "後方一致" => "backword", "部分一致" => "partial"}) %>
    <%= f.submit "検索" %>
  <% end %>
<% end %>